<Window x:Class="Vistas.SectoresOcupados"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" Icon="/Vistas;component/Images/logoParking.png" Style="{StaticResource MyWindowDefault}"
        Title="Sectores Ocupados" Height="416" Width="619"  WindowStartupLocation="CenterScreen">
    <Grid>
        <StackPanel Margin="20">
            <TextBlock Text="Listado de Sectores Ocupados" HorizontalAlignment="Center" FontSize="20" Margin="0,0,0,20" />
            <ScrollViewer VerticalScrollBarVisibility="Visible">
                <DataGrid x:Name="dgSectoresOcupados" AutoGenerateColumns="True" CanUserAddRows="False" IsReadOnly="True" Height="239" Width="553" />
            </ScrollViewer>
            <Button Content="Imprimir Listado" Click="btnImprimirListado_Click" Width="150" Margin="0,20,0,0"  Style="{StaticResource ButtonStyle}"/>
        </StackPanel>
    </Grid>
</Window>
